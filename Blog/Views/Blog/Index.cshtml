@model IEnumerable<BlogDB>

<h2>Blogs</h2>

<ul>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
    @foreach (var blog in Model)
    {
        <li>@blog.Url</li>
        <li>@blog.Name</li>

    }
</ul>
